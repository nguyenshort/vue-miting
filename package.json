{
  "name": "it-square",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 && vite build",
    "preview": "vite preview",
    "check:ts": "vue-tsc --noEmit && vite build",
    "build:env": "sed 's/=.*/=/' .env > .env.example",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "apollo:codegen": "npx apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --tagName=gql --includes=./src/apollo/it/**/*.ts --globalTypesFile=src/apollo/__generated__/itTypes.ts",
    "apollo:download": "npx apollo client:download-schema --endpoint=https://it-be-v2.smileeye.edu.vn/graphql schema.graphql",
    "restart": "pm2 restart it-square",
    "stop": "pm2 stop it-square"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@formkit/auto-animate": "^1.0.0-beta.1",
