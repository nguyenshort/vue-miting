    "firebase": "^9.9.2",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.8.2",
    "pinia": "^2.0.14",
    "sass": "^1.54.4",
    "string-template": "^1.0.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "uuid": "^8.3.2",
    "vant": "^3.6.0",
    "vue": "^3.2.37",
    "vue-cookies": "^1.8.1",
    "vue-router": "^4.0.16",
    "vue3-grid-layout": "^1.0.0"
